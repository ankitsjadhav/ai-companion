generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

// prisma/schema.prisma
datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
  // uncomment next line if you use Prisma <5.10
  // directUrl = env("DATABASE_URL_UNPOOLED")
}

model Category {
  id   String @id @default(uuid())
  name String
}